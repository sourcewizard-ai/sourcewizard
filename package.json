{
  "name": "repowizard",
  "version": "1.0.0",
  "description": "MCP Package Manager with AI-powered guidance",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:ink": "npx babel src/cli/ink-ui-minimal.tsx --out-file dist/ink-ui-minimal.js && npx babel src/cli/ink-demo.mts --out-file dist/ink-demo.js",
    "start": "node dist/index.js",
    "dev": "tsx src/cli/demo.ts",
    "dos-ui": "tsx src/cli/dos-ui.ts",
    "ink-ui": "node --loader ts-node/esm src/cli/ink-ui-cli.ts",
    "ink-ui-simple": "tsx src/cli/ink-demo-simple.ts",
    "ink-ui-minimal": "node --import tsx/esm src/cli/ink-demo-minimal.mts"
  },
  "keywords": [
    "mcp",
    "package-manager",
    "ai",
    "cli",
    "terminal-ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "figlet": "^1.7.0",
    "fullscreen-ink": "^0.0.2",
    "gradient-string": "^2.0.2",
    "ink": "^6.0.1",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^4.0.3",
    "react": "^19.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/ink-big-text": "^1.2.4",
    "@types/ink-gradient": "^2.0.4",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
