{
  "name": "sourcewizard",
  "version": "0.3.1",
  "description": "SourceWizard - AI-powered setup wizard for dev tools and libraries with MCP integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sourcewizard": "dist/cli/index.js"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node scripts/fix-imports.js",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development tsx cli/index.ts",
    "mcp": "tsx mcp/server.ts",
    "test": "jest --no-watch",
    "test:coverage": "jest --coverage",
    "build-registry": "npx @typeconf/typeconf@latest build registry",
    "push-registry": "tsx scripts/insert-registry-configs.ts",
    "push-registry:staging": "tsx scripts/insert-registry-configs.ts --staging",
    "push-registry:dry-run": "tsx scripts/insert-registry-configs.ts --dry-run"
  },
  "keywords": [
    "sourcewizard",
    "package-manager",
    "ai",
    "agent",
    "cli",
    "terminal-ui",
    "code-snippets",
    "developer-tools"
  ],
  "author": "Ivan Chebykin <ivan@chebykin.org>",
  "license": "Apache-2.0",
  "homepage": "https://sourcewizard.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcewizard-ai/sourcewizard.git",
    "directory": "sourcewizard"
  },
  "bugs": {
    "url": "https://github.com/sourcewizard-ai/sourcewizard/issues"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@supabase/supabase-js": "^2.58.0",
    "@typeconf/sdk": "^0.3.2",
    "@types/commander": "^2.12.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/ora": "^3.2.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "figlet": "^1.9.3",
    "fullscreen-ink": "^0.0.2",
    "glob": "^11.0.3",
    "gradient-string": "^2.0.2",
    "ink": "^6.3.1",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^6.0.0",
    "open": "^10.2.0",
    "ora": "^8.2.0",
    "react": "^19.2.0",
    "ts-node": "^10.9.2",
    "zod": "^3.25.76",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/figlet": "^1.7.0",
    "@types/ink-big-text": "^1.2.4",
    "@types/ink-gradient": "^2.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
