{
  "name": "sourcewizard",
  "version": "0.1.0",
  "description": "SourceWizard - AI-powered setup wizard for dev tools and libraries with MCP integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sourcewizard": "dist/src/cli/index.js"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/cli/index.ts",
    "mcp": "tsx src/mcp/server.ts",
    "ink-ui": "node --loader ts-node/esm src/cli/ink-ui-cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "import-registry": "tsx scripts/insert-registry-configs.ts",
    "import-registry:dry-run": "tsx scripts/insert-registry-configs.ts --dry-run"
  },
  "keywords": [
    "sourcewizard",
    "package-manager",
    "ai",
    "agent",
    "cli",
    "terminal-ui",
    "code-snippets",
    "developer-tools"
  ],
  "author": "Ivan Chebykin <ivan@chebykin.org>",
  "license": "Apache-2.0",
  "homepage": "https://sourcewizard.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcewizard-ai/sourcewizard.git",
    "directory": "sourcewizard"
  },
  "bugs": {
    "url": "https://github.com/sourcewizard-ai/sourcewizard/issues"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@inkjs/ui": "^2.0.0",
    "@modelcontextprotocol/sdk": "^1.1.0",
    "@supabase/supabase-js": "^2.52.0",
    "@typeconf/sdk": "^0.3.2",
    "@types/commander": "^2.12.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/ora": "^3.1.0",
    "ai": "^4.3.19",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "figlet": "^1.7.0",
    "fullscreen-ink": "^0.0.2",
    "glob": "^11.0.3",
    "gradient-string": "^2.0.2",
    "ink": "^6.1.0",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^6.0.0",
    "open": "^10.2.0",
    "ora": "^8.2.0",
    "react": "^19.1.0",
    "ts-node": "^10.9.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/figlet": "^1.5.6",
    "@types/ink-big-text": "^1.2.4",
    "@types/ink-gradient": "^2.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
